{
  "name": "env-audit",
  "version": "1.0.0",
  "description": "🔍 Scan your codebase for environment variables and generate .env.example files automatically. Supports Next.js, Vite, Astro, SvelteKit, Docker, and more.",
  "keywords": [
    "environment",
    "env",
    "dotenv",
    "configuration",
    "nextjs",
    "vite",
    "astro",
    "sveltekit",
    "docker",
    "github-actions",
    "cli",
    "typescript",
    "ast",
    "static-analysis"
  ],
  "homepage": "https://github.com/vaporjawn/env-audit#readme",
  "bugs": {
    "url": "https://github.com/vaporjawn/env-audit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaporjawn/env-audit.git"
  },
  "license": "MIT",
  "author": {
    "name": "Victor Williams",
    "email": "vaporjawn@gmail.com",
    "url": "https://github.com/vaporjawn"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "env-audit": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsx src/cli/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage",
    "prepack": "npm run clean && npm run build",
    "prepublishOnly": "npm run test:run && npm run lint && npm run typecheck",
    "release": "npm run prepack && npm publish",
    "release:dry": "npm run prepack && npm publish --dry-run"
  },
  "dependencies": {
    "@babel/generator": "^7.25.6",
    "@babel/parser": "^7.25.6",
    "@babel/traverse": "^7.25.6",
    "@babel/types": "^7.25.6",
    "commander": "^12.1.0",
    "fast-glob": "^3.3.2",
    "picocolors": "^1.1.0",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^20.16.5",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
